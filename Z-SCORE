Z-SCORE NORMALIZATION
# Z-Score Normalization Function
z_score_normalization <- function(data) {
  return((data - mean(data)) / sd(data))
}

# Example Usage
data_vector <- c(10, 20, 30, 40, 50)
normalized_data <- z_score_normalization(data_vector)
print(normalized_data)


MEDIAN
# Median Calculation in R

# Sample data
data <- c(1, 3, 3, 6, 7, 8, 9)

# Calculate median
median_value <- median(data)

# Print the median
print(median_value)


MINIMUM
# Minimum Code Example
x <- c(1, 2, 3, 4, 5)
mean_x <- mean(x)
print(mean_x)


MAXIMUM
# Maximum Code

# Function to find the maximum value in a numeric vector
find_maximum <- function(vec) {
  if (length(vec) == 0) {
    return(NA)
  }
  max_value <- vec[1]
  for (value in vec) {
    if (value > max_value) {
      max_value <- value
    }
  }
  return(max_value)
}

# Example usage
numeric_vector <- c(3, 5, 1, 8, 2)
maximum_value <- find_maximum(numeric_vector)
print(maximum_value)


MINMAX
# Min-Max Normalization

# Sample data
data <- c(10, 20, 30, 40, 50)

# Min-Max Normalization function
minmax_normalization <- function(x) {
  return ((x - min(x)) / (max(x) - min(x)))
}

# Applying the function
normalized_data <- minmax_normalization(data)

# Displaying the normalized data
print(normalized_data)



