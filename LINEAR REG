MEAN
calculate_mean <- function(numbers) {
  mean_value <- mean(numbers)
  return(mean_value)
}

# Sample data
numbers <- c(10, 20, 30, 40, 50)

# Calculate mean
mean_result <- calculate_mean(numbers)

# Print result
cat("The mean of the given numbers is:", mean_result, "\n")
